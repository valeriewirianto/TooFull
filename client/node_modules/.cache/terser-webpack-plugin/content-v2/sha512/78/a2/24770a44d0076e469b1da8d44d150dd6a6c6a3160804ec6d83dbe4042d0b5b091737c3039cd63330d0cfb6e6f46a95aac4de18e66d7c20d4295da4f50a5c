 implemented yet\");//event.preventDefault();\nsignupFetch(this,event.target);alert('after');}},{key:\"render\",value:function render(){var _this6=this;return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{children:[/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"form\",{onSubmit:this.handleSubmit,children:[/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"input\",{type:\"text\",name:\"usernameInput\",className:\"input\",value:this.state.usernameInput,onChange:this.handleChange,placeholder:\"Username\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"input\",{type:\"password\",name:\"passwordInput\",className:\"input\",value:this.state.passwordInput,onChange:this.handleChange,placeholder:\"Password\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"input\",{type:\"password\",name:\"confirmedpasswordInput\",className:\"input\",value:this.state.confirmedpasswordInput,onChange:this.handleChange,placeholder:\"Confirm Password\"}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"br\",{}),/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(\"input\",{className:\"submitButton\",type:\"submit\",value:\"Sign Up\"})]}),/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"div\",{className:\"backLogIn\",children:[\" \",/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(Login_LogInBar,{handleClick:function handleClick(){return _this6.props.handleClick();}}),\" \"]})]});}}]);return SignUpForm;}(react_default.a.Component);var Login_LogInBar=/*#__PURE__*/function(_React$Component6){Object(inherits[\"a\" /* default */])(LogInBar,_React$Component6);var _super6=Object(createSuper[\"a\" /* default */])(LogInBar);function LogInBar(){Object(classCallCheck[\"a\" /* default */])(this,LogInBar);return _super6.apply(this,arguments);}Object(createClass[\"a\" /* default */])(LogInBar,[{key:\"render\",value:function render(){return/*#__PURE__*/Object(jsx_runtime[\"jsxs\"])(\"label\",{children:[\" Already have an account?\",/*#__PURE