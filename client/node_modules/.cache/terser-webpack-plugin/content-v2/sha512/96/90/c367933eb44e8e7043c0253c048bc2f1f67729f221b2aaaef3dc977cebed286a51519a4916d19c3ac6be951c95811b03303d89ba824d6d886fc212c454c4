ob\", \"Cathrine\", \"George\", 'Mia']\n    }\n\n    deleteUser = (username) => {\n        const filterUsers= this.state.AllUsers.filter(\n            (check) =>{\n                return check !== username\n            }\n        )\n        this.setState(\n            {\n                AllUsers: filterUsers\n            }\n        )\n    }\n\n    AllUsersRow(username){\n        return(\n            <tr key={uid(username)}>\n                <td>\n                    <Link to={'/Profile'}>\n                        <button type=\"button\" className='UsernameButton'> {username}  </button>\n                    </Link>\n                </td>\n                <td>\n                    <button type=\"button\" className='DeleteFriend'\n                            onClick = {\n                                () => this.deleteUser(username)\n                            }\n                    > delete </button>\n                </td>\n            </tr>\n        )\n    }\n\n\n    AllUsersTable(){\n        return (\n            <table id='AllUsersTable'>\n                <tbody>\n                <tr>\n                    <th>username</th>\n                    <th>Delete</th>\n                </tr>\n                {this.state.AllUsers.map(username => {return this.AllUsersRow(username)})}\n                </tbody>\n            </table>\n        )\n    }\n\n    AllUsersPanel1(){\n        return(\n            <div id='AllUsersPanel1'>\n                <h1> All the users:</h1>\n                {this.AllUsersTable()}\n            </div>\n        )\n    }\n\n    AllUsersPanel2(){\n        return(\n            <div id=\"AllUsersPanel2\">\n                <p>\n                    <strong>Dear Admin</strong>\n                    <br />\n                    Deleting a user is not good.\n                    <br />\n                    Think about the decision you make carefully before doing it.\n                    <br />\n                    We have warned you!\n                </p>\n    