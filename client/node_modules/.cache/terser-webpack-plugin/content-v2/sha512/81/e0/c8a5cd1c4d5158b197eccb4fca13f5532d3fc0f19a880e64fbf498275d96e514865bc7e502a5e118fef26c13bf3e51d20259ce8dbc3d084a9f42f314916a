Friends= this.state.PotentialFriendsList.filter(\n            (check) =>{\n                return check !== username\n            }\n        )\n        let Friends = this.state.ExistingFriendsList\n\n        Friends.push(username)\n        this.setState(\n            {\n                PotentialFriendsList: filterFriends,\n                ExistingFriendsList: Friends\n            }\n        )\n    }\n\n    PotentialFriendsRow(username){\n        return(\n            <tr key={uid(username)}>\n                <td>\n                    <Link to={'/Profile'}>\n                        <button type=\"button\" className='UsernameButton'> {username}  </button>\n                    </Link>\n                </td>\n                <td>\n                    <button type=\"button\" className='AcceptFriend'\n                            onClick={\n                                () => this.acceptFriend(username)\n                            }\n                    > Accept! </button>\n                </td>\n                <td>\n                    <button type=\"button\" className='RejectFriend'\n                            onClick={\n                                () => this.rejectFriend(username)\n                            }\n                    > NO! </button>\n                </td>\n            </tr>\n        )\n    }\n\n\n    PotentialFriends(){\n        return (\n            <table id=\"PotentialFriends\">\n                <tbody>\n                <tr>\n                    <th>username</th>\n                    <th>Accept</th>\n                    <th>Reject</th>\n                </tr>\n                {this.state.PotentialFriendsList.map(username => {return this.PotentialFriendsRow(username)})}\n                </tbody>\n            </table>\n        )\n    }\n\n\n    Panel2(){\n        return(\n            <div id=\"FriendsPanel2\">\n                <h1>Who want to be my friends</h1>\n                {this.PotentialFriends()}\n            </div>\n        )\n    }\n\n\n\